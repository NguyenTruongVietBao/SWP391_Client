// src/index.ts
function createThunkMiddleware(extraArgument) {
  const middleware = ({
    dispatch,
    getState
  }) => next => action => {
    if (typeof action === "function") {
      return action(dispatch, getState, extraArgument);
    }
    return next(action);
  };
  return middleware;
}
var thunk = createThunkMiddleware();
var withExtraArgument = createThunkMiddleware;


